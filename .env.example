########################################
# Network & Builder Configuration
########################################
# Mainnet: 1, Sepolia: 11155111
CHAIN_ID=1

# HTTPS RPC endpoint
NORMAL_RPC=
# WebSocket RPC endpoint (optional). If omitted, derived from NORMAL_RPC
WEBSOCKET_RPC=

# Alchemy API Key (RECOMMENDED: Prevents rate limits during mempool monitoring)
# Get yours at: https://www.alchemy.com/
# Enables filtered pending transaction subscriptions for efficient monitoring
ALCHEMY_API_KEY=

# Flashbots relay RPC (supports mainnet and sepolia if provided by operator)
FLASHBOTS_RPC=https://relay.flashbots.net

# BeaverBuild RPC (alternative builder)
BEAVER_RPC_URL=https://rpc.beaverbuild.org/

# Toggle which builder to use (Flashbots vs BeaverBuild)
USE_FLASHBOTS=true

# Run simulation only (no broadcast). Recommended true first
SIMULATE=false

########################################
# Wallet Keys
########################################
# Private key of wallet with ETH for gas
FUNDER_PRIVATE_KEY=

# Private key of compromised wallet (the script signs with this for recovery)
COMPROMISED_PRIVATE_KEY=

########################################
# Contracts & Safe Setup
########################################
# ERC20 proxy (or token) address to recover
ERC20_TOKEN_ADDRESS=

# If the token is behind a proxy, the proxy address (defaults to ERC20_TOKEN_ADDRESS if omitted)
PROXY_CONTRACT_ADDRESS=

# ProxyAdmin address (owner should be the Safe). Required by orchestrator
PROXY_ADMIN_ADDRESS=

# Safe (Gnosis Safe) address controlling the ProxyAdmin. Required by orchestrator
SAFE_ADDRESS=

# Safe Transaction Service base URL per network
# Mainnet:   https://safe-transaction-mainnet.safe.global
# Sepolia:   https://safe-transaction-sepolia.safe.global
SAFE_API_BASE_URL=https://safe-transaction-mainnet.safe.global

########################################
# Gas & Funding
########################################
# Amount of ETH to fund compromised wallet (as string ETH units)
ETH_AMOUNT_TO_FUND=0.002

# Base gas price (gwei). MaxFee is computed as base*3 + tip
BASE_GAS_PRICE=4

# Priority fee (gwei)
TIP_IN_GWEI=10

COMPROMISED_PUBLIC_KEY=